                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="400.0" prefWidth="0.0">
         <children>
            <Accordion fx:id="Menu" prefHeight="400.0" prefWidth="200.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <panes>
                <TitledPane animated="false" text="Online">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <ListView fx:id="OnlineList" layoutX="-1.0" layoutY="-2.0" prefHeight="350.0" prefWidth="200.0" />
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
                <TitledPane animated="false" text="Friends">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <ListView fx:id="FriendList" layoutX="-1.0" prefHeight="350.0" prefWidth="200.0" />
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
                <TitledPane animated="false" text="Setiings">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="350.0" prefWidth="200.0">
                           <children>
                              <TextField fx:id="SocketInputArea" layoutX="23.0" layoutY="13.0" />
                              <Button fx:id="ConnectButton" layoutX="66.0" layoutY="49.0" mnemonicParsing="false" onAction="#connect" text="Connect" />
                              <CheckBox fx:id="CWSOptionButton" layoutX="15.0" layoutY="88.0" mnemonicParsing="false" text="Connect with start the app" />
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
              </panes>
            </Accordion>
         </children>
      </AnchorPane>
   </children>
